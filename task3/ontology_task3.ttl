@prefix : <http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/> .

<http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/includes
:includes rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/has_employee
:has_employee rdf:type owl:ObjectProperty .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/accesses_resource
:accesses_resource rdf:type owl:ObjectProperty .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/works_on_project
:works_on_project rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/ValidAccess
:ValidAccess rdf:type owl:Class .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/GameProject
:GameProject rdf:type owl:Class .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/ResourceAccess
:ResourceAccess rdf:type owl:Class .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/Department
:Department rdf:type owl:Class .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/Employee
:Employee rdf:type owl:Class .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/GameResource
:GameResource rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/John_Doe
:John_Doe rdf:type owl:NamedIndividual ,
                   :Employee ;
          :works_on_project :Mobile_RPG_Game .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/ImageLibrary
:ImageLibrary rdf:type owl:NamedIndividual ,
                       :GameResource .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/Mobile_RPG_Game
:Mobile_RPG_Game rdf:type owl:NamedIndividual ,
                          :GameProject ;
                 :includes :DesignDepartment .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/DesignDepartment
:DesignDepartment rdf:type owl:NamedIndividual ,
                           :Department ;
                  :includes :ImageLibrary .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/access_1
:access_1 rdf:type owl:NamedIndividual ,
                   :ResourceAccess ;
          :has_employee :John_Doe ;
          :accesses_resource :ImageLibrary .
