#    Individuals
#################################################################

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/John_Doe
:John_Doe rdf:type owl:NamedIndividual ,
                   :Employee ;
          :works_on_project :Mobile_RPG_Game .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/ImageLibrary
:ImageLibrary rdf:type owl:NamedIndividual ,
                       :GameResource .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/Mobile_RPG_Game
:Mobile_RPG_Game rdf:type owl:NamedIndividual ,
                          :GameProject ;
                 :includes :DesignDepartment .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/DesignDepartment
:DesignDepartment rdf:type owl:NamedIndividual ,
                           :Department ;
                  :includes :ImageLibrary .

###  http://kik.misis.ru/ontology/mpi/2024/Фам-Минь-Хиеу/task3/access_1
:access_1 rdf:type owl:NamedIndividual ,
                   :ResourceAccess ;
          :has_employee :John_Doe ;
          :accesses_resource :ImageLibrary .


